# Application
NODE_ENV=production
BFF_VERSION=latest
BFF_PORT=3000

# PostgreSQL
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=cv_builder

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
